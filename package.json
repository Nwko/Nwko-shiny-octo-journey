{
  "name": "soh",
  "version": "1.0.0",
  "description": "Oh, look a shiny octo journey :D",
  "scripts": {
    "start": "parcel src/pages/* --dist-dir web",
    "dev": "parcel serve src/pages/* --dist-dir www",
    "build": "parcel build src/pages/*",
    "deploy": "./Deploy.sh"
  },
  "targets": {
    "frontend": {
      "source": "src/pages/*"
    },
    "default": {
      "distDir": "./public"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nwko/shiny-octo-journey.git"
  },
  "author": "Nox",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/Nwko/shiny-octo-journey/issues"
  },
  "homepage": "https://github.com/Nwko/shiny-octo-journey#readme",
  "devDependencies": {
    "@parcel/transformer-sass": "2.4.1",
    "parcel": "^2.4.1",
    "sass": "^1.49.10"
  }
}
